import express from 'express';
import fetch from 'node-fetch';

const app = express();
app.use(express.json());

//set app view engine
app.set('view engine', 'ejs');

app.get('/', (req, res) => {
  res.send('This is a test!');
});

app.listen(3000, () => {
  console.log('App is listening to port 3000...');
});
